{% extends 'GesEnquiryBundle::default_layout.html.twig' %}

{% block body -%}
    <h1>Enquiry creation</h1>
    <a href="{{ path('enquiry') }}" class="btn">
        Back to the list
    </a>
    <hr>
    <form action="{{ path('enquiry_create') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <p>
            <button type="submit" class="btn">Create Enquiry</button>
        </p>
    </form>

{% endblock %}
