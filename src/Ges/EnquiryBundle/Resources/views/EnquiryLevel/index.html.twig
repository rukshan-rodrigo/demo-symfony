{% extends 'GesEnquiryBundle::default_layout.html.twig' %}

{% block body -%}
    <h1>Enquiry Level list</h1>
    <a href="{{ path('enquirylevel_new') }}" class="btn">
        Create a new Enquiry Level
    </a>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Sequence</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('enquirylevel_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.name }}</td>
                <td>{{ entity.sequence }}</td>
                <td>
                    <a href="{{ path('enquirylevel_show', { 'id': entity.id }) }}">View</a> |
                    <a href="{{ path('enquirylevel_edit', { 'id': entity.id }) }}">Edit</a>

                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>


    {% endblock %}
